{
  "name": "sme-prova-serap-acompanhamento-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src",
    "prettier": "npx prettier src --check",
    "test": "jest --coverage",
    "test2": "jest --clearCache"
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "antd": "^4.22.3",
    "axios": "^0.27.2",
    "immer": "^9.0.15",
    "moment": "^2.29.4",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@reduxjs/toolkit": "^2.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/antd": "^1.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.2.5",
    "ts-jest-mock-import-meta": "^1.2.1",
    "typescript": "^4.9.5",
    "vite": "^3.2.11",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-imp": "^2.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "coverageDirectory": "coverage",
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "useESM": true,
          "diagnostics": {
            "ignoreCodes": [
              1343
            ]
          },
          "astTransformers": {
            "before": [
              {
                "path": "ts-jest-mock-import-meta",
                "options": {
                  "metaObjectReplacement": {
                    "VITE_SME_SERAP_ACOMPANHAMENTO_API": "https://example.com",
                    "VITE_SME_SERAP": "https://example.com"
                  }
                }
              }
            ]
          }
        }
      ],
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/test-utils.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!your-module-to-transform).+\\.js$"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    }
  }
}
